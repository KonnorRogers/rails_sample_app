<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<p>Username: <%= @user.name %></p>
<p>Email: <%= @user.email %></p>
<p>Created_at: <%= @user.created_at %></p>
<p>Updated_at: <%= @user.updated_at %></p>

<p>Current_time: <%= Time.now %></p>
